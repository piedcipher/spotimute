{
    "name": "Spotimute",
    "version": "0.0.1",
    "description": "This extension mutes the spotify & gaana audio when ad comes.",
    "manifest_version": 3,
    "author": "Tirth Patel",
    "action": {
        "default_popup": "index.html",
        "default_title": "Spotimute"
    },
    "content_scripts": [
        {
            "matches": ["https://open.spotify.com/*"],
            "js": ["spotify.js"]
        },
        {
            "matches": ["https://gaana.com/*"],
            "js": ["gaana.js"]
        }
    ],
    "icons": {
        "16": "./assets/_16.png",
        "48": "./assets/_48.png",
       "128": "./assets/_128.png"
    }
}